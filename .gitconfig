[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=tformat:\"%h %ad %Cgreen[%an]%Creset\t %Cblue%s %Cred%d%Creset\" --graph --date=short -n 30
  amend = commit --all --amend --no-edit
  type = cat-file -t
  dump = cat-file -p
[core]
	pager = cat
[push]
	default = matching
[color]
  diff = auto
  status = auto
  branch = auto
  interactive = auto
  ui = auto
  pager = true
